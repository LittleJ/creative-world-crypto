{
    "name": "creative-world/crypto",
    "description": "A PHP package for cryptography management using the CryptoModel class.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/creative-world/crypto",
    "authors": [
        {
            "name": "Johan HAUDIQUET",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "ext-sodium": "*"
    },
    "autoload": {
        "psr-4": {
            "CreativeWorld\\Crypto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CreativeWorld\\Crypto\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    },
    "keywords": [
        "crypto",
        "cryptography",
        "encryption",
        "security"
    ]
}